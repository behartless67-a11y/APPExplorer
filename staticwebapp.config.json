{
  "routes": [
    {
      "route": "/api/*",
      "allowedRoles": ["anonymous"]
    }
  ],
  "navigationFallback": {
    "rewrite": "/index.html",
    "exclude": ["/api/*", "*.{css,scss,sass,js,ts,map,ico,png,jpg,gif,svg,eot,otf,ttf,woff,woff2}"]
  },
  "mimeTypes": {
    ".json": "application/json"
  }
}